		if(grpElem.length>0){
			for(var i=0;i<grpElem.length;i++){					
				if(grpElem[i].checked){				
					cnt++;					
				}					
			}			
		}
		else{
			if(grpElem.checked){				
				cnt++;		
			}			
		}
		if(cnt>=chkNum){
			return true;
		}
		else{
			return false;
		}
	}



