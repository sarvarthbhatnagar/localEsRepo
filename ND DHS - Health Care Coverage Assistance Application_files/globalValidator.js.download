	function encrypt(number){
		  var encrypted, temp;
		  var first, second, third, fourth;
		  
		   temp = number;
		  first=(temp/1000+7)%10;
		  temp=temp%1000;
		  second=(temp/100+7)%10;
		  temp=temp%100;
		  third=(temp/10+7)%10;
		  temp=temp%10;
		  fourth=(temp+7)%10;

		encrypted=Math.floor(third)*1000+Math.floor(fourth)*100+Math.floor(first)*10+Math.floor(second);
		return encrypted;
	}

