function trimTextBoxData(){
	$("input[maxlength='10']").each(function() {
		try{
			var id  = $(this.value);
			var value = id.selector;
			if(value.length > 10){
				var trimmedValue = value.substring(0,value.length-3);
				$(this).val(trimmedValue);
			}
		}catch (e) {
			console.error('Exception occured in trimTextBoxData()');
		}
	});
}
