	var dt = d.value;

	if( '/' == dt.charAt(1)){
		dt = '0'+dt;
	}else if( '/' == dt.charAt(3)){
		dt = dt.substr(0,2);
	}

	var da = dt.split('/');
	for (var a = 0; a < da.length; a++) {
		if (da[a] != +da[a]){
			da[a] = da[a].substr(0,da[a].length-1);
		}
	}
	if (da[0] > 12) {
		da[1] = da[0].substr(da[0].length-1,1);
		da[0] = '0'+da[0].substr(0,da[0].length-1);
	}
	if (da[1] > 9999){
		da[1] = da[1].substr(0,da[1].length-1);
	}
